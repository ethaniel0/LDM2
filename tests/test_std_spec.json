  {
    "type": "structure",
    "name": "make_variable_standard",
    "components": [
      {
        "base": "typename",
        "name": "type"
      },
      {
        "base": "name",
        "name": "varname",
        "type": "new-local"
      },
      {
        "base": "expression",
        "name": "expr"
      }
    ],
    "create_variable": {
      "name": "$varname",
      "type": "$type",
      "check_type": "$expr",
      "scope": "local"
    }
  },

  {
    "type": "structure",
    "name": "main_block",
    "components": [
      {
        "base": "expressions",
        "name": "body",
        "allow": "all",
        "scope": "local"
      }
    ],
    "dependent": true
  },

  {
    "type": "structure",
    "name": "function",
    "components": [
      {
        "base": "typename",
        "name": "type"
      },
      {
        "base": "name",
        "name": "varname",
        "type": "new-local"
      },
      {
        "base": "repeated_element",
        "name": "arguments",
        "components": [
            {
                "base": "typename",
                "name": "type"
            },
            {
                "base": "name",
                "name": "varname",
                "type": "new-local"
            }
        ]
      },
      {
        "base": "structure",
        "name": "body",
        "structure": "main_block",
        "modifiers": {
          "body": {
            "scope": "local",
            "insert_scope": [
              {
                "type": "$arguments.type",
                "name": "$arguments.varname"
              }
            ]
          }
        }
      }
    ],
    "create_variable": {
      "type": "function<$typename>",
      "name": "$varname",
      "scope": "local",
      "attributes": {
        "arguments": "$arguments"
      }
    }
  },

  {
    "type": "structure",
    "name": "struct_initialize_variable",
    "components": [
      {
        "base": "typename",
        "name": "type"
      },
      {
        "base": "name",
        "name": "varname",
        "type": "new-local"
      }
    ],
    "create_variable": {
      "name": "$varname",
      "type": "$type",
      "scope": "local"
    },
    "dependent": true
  },

  {
    "type": "structure",
    "name": "struct",
    "components": [
      {
        "base": "name",
        "name": "struct_name",
        "type": "new-global"
      },
      {
        "base": "structure",
        "name": "body",
        "structure": "main_block",
        "modifiers": {
          "body": {
            "scope": "local",
            "filter": [
              {
                "type": "structure",
                "structure": "struct_initialize_variable"
              }
            ]
          }
        }
      }
    ],
    "create_type": {
      "type": "$struct_name",
      "scope": "global",
      "fields_containers": ["body.body"]
    }

  },

  {
    "type": "structure",
    "name": "if",
    "components": [
      {
        "base": "expression",
        "name": "condition"
      },
      {
        "base": "structure",
        "name": "body",
        "structure": "main_block",
        "modifiers": {
          "body": {
            "scope": "global"
          }
        }
      }
    ]
  },

  {
    "type": "value_keyword",
    "name": "true",
