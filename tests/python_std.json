[
  {
    "type": "expression_separator",
    "name": "main",
    "translate": "\n"
  },
  {
    "type": "value_keyword",
    "name": "true",
    "translate": "True"
  },
  {
    "type": "value_keyword",
    "name": "false",
    "translate": "False"
  },

  {
    "type": "primitive_type",
    "name": "bool",
    "translate": "bool"
  },
  {
    "type": "primitive_type",
    "name": "int",
    "translate": "int"
  },
  {
    "type": "primitive_type",
    "name": "int8",
    "translate": "int"
  },
  {
    "type": "primitive_type",
    "name": "float",
    "translate": "float"
  },

  {
    "type": "make_variable",
    "name": "standard",
    "translate": "$varname: $type = $expr"
  },

  {
    "type": "make_object",
    "name": "function",
    "translate": "def $varname ( $arguments ) $body",
    "component_structures": {
      "arguments": {
        "translate": "$varname: $type",
        "separator": ", "
      }
    }
  },

  {
    "type": "block",
    "name": "main",
    "translate": ":\n$body",
    "indent": 4
  },

  {
    "type": "keyword",
    "name": "if",
    "translate": "if $condition $body"
  },

  {
    "type": "operator",
    "name": "+",
    "translate": "$left + $right"
  },

  {
    "type": "operator_overload",
    "name": "+",
    "left": "int", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "+",
    "left": "float", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "+",
    "left": "int", "right": "float"
  },
  {
    "type": "operator_overload",
    "name": "+",
    "left": "float", "right": "int"
  },

  {
    "type": "operator",
    "name": "-",
    "translate": "$left - $right"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "int", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "float", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "int", "right": "float"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "float", "right": "int"
  },

  {
    "type": "operator",
    "name": "neg",
    "translate": "- $right"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "int", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "-",
    "left": "float", "right": "float"
  },

  {
    "type": "operator",
    "name": "*",
    "translate": "$left * $right"
  },
  {
    "type": "operator_overload",
    "name": "*",
    "left": "int", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "*",
    "left": "float", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "*",
    "left": "int", "right": "float"
  },
  {
    "type": "operator_overload",
    "name": "*",
    "left": "float", "right": "int"
  },

  {
    "type": "operator",
    "name": "?:",
    "translate": "$middle if $left else $right"
  },
  {
    "type": "operator_overload",
    "name": "?:",
    "left": "bool",
    "middle": "$typename<T>",
    "right": "$typename<T>"
  },

  {
    "type": "operator",
    "name": "=",
    "translate": "$left = $right"
  },
  {
    "type": "operator_overload",
    "name": "=",
    "left": "$typename<T>",
    "right": "$typename<T>"
  },


  {
    "type": "operator",
    "name": "<",
    "translate": "$left < $right"
  },
  {
    "type": "operator_overload",
    "name": "<",
    "left": "int", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "<",
    "left": "float", "right": "int"
  },
  {
    "type": "operator_overload",
    "name": "<",
    "left": "int", "right": "float"
  },
  {
    "type": "operator_overload",
    "name": "<",
    "left": "float", "right": "int"
  }
]