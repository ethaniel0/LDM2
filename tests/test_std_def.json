[
  {
    "type": "structure",
    "name": "make_variable_standard",
    "structure": "$type $varname = $expr"
  },

  {
    "type": "structure",
    "name": "main_block",
    "structure": "{ $body }"
  },


  {
    "type": "structure",
    "name": "function",
    "structure": "$type $varname ( $arguments ) $body",
    "component_structures": {
      "arguments": {
        "structure": "$type $varname",
        "separator": ","
      }
    }
  },

  {
    "type": "expression_separator",
    "name": "semicolon",
    "value": ";"
  },


  {
    "type": "structure",
    "name": "if",
    "structure": "if ( $condition ) $body"
  },

  {
    "type": "structure",
    "name": "struct_initialize_variable",
    "structure": "$type $varname ;"
  },

  {
    "type": "structure",
    "name": "+",
    "structure": "$left + $right",
    "create_operator": {
      "precedence": 6,
      "associativity": "left-to-right"
    }
  },

  {
    "type": "structure",
    "name": "struct",
    "structure": "struct $struct_name $body"
  },


  {
    "type": "value_keyword",
    "name": "true",
    "typed_name": "true"
  },
  {
    "type": "value_keyword",
    "name": "false",
    "typed_name": "false"
  }
]
