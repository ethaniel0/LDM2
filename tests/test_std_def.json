[
  {
    "type": "make_variable",
    "name": "standard",
    "structure": "$typename $varname = $expr"
  },

  {
    "type": "operator",
    "name": "+",
    "structure": "$left + $right",
    "associativity": "left-to-right"
  },

  {
    "type": "operator",
    "name": "-",
    "structure": "$left - $right",
    "associativity": "left-to-right"
  },

  {
    "type": "operator",
    "name": "neg",
    "structure": "- $right",
    "associativity": "left-to-right"
  },

  {
    "type": "operator",
    "name": "*",
    "structure": "$left * $right",
    "associativity": "left-to-right"
  },

  {
    "type": "operator",
    "name": "?:",
    "structure": "$left ? $middle : $right",
    "associativity": "left-to-right"
  }
]
