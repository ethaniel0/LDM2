[
  {
    "type": "structure",
    "name": "make_variable_standard",
    "structure": "$type $varname = $expr"
  },

  {
    "type": "structure",
    "name": "main_block",
    "structure": "{ $body }"
  },


  {
    "type": "structure",
    "name": "function",
    "structure": "$type $varname ( $arguments ) $body",
    "component_structures": {
      "arguments": {
        "structure": "$type $varname",
        "separator": ","
      }
    }
  },

  {
    "type": "expression_separator",
    "name": "semicolon",
    "value": ";"
  },


  {
    "type": "structure",
    "name": "if",
    "structure": "if ( $condition ) $body"
  },

  {
    "type": "value_keyword",
    "name": "true",
    "typed_name": "true"
  },
  {
    "type": "value_keyword",
    "name": "false",
    "typed_name": "false"
  },


  {
    "type": "operator",
    "name": "+",
    "precedence": 6,
    "structure": "$left + $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "-",
    "precedence": 6,
    "structure": "$left - $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "neg",
    "precedence": 3,
    "structure": "- $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "*",
    "precedence": 5,
    "structure": "$left * $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "?:",
    "precedence": 16,
    "structure": "$left ? $middle : $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "()",
    "precedence": 0,
    "structure": "( $inside )",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "<",
    "precedence": 9,
    "structure": "$left < $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": ">",
    "precedence": 9,
    "structure": "$left > $right",
    "associativity": "left-to-right"
  },
  {
    "type": "operator",
    "name": "=",
    "precedence": 16,
    "structure": "$left = $right",
    "associativity": "left-to-right"
  }
]
